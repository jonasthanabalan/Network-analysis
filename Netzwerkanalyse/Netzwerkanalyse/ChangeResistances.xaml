<Window x:Class="Netzwerkanalyse.ChangeResistances"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Netzwerkanalyse"
        mc:Ignorable="d"
        Title="ChangeResistances" Height="540" Width="400" ResizeMode="NoResize">
    <StackPanel Margin="0, 20">
        <Label Content="R1 :" Width="30"/>
        <TextBox x:Name="R1" Height="25" Width="100"/>
        <Label Content="R2 :" Width="30" Margin="20"/>
        <TextBox x:Name="R2" Height="25" Width="100"/>
        <Label Content="R3 :" Width="30" Margin="20"/>
        <TextBox x:Name="R3" Height="25" Width="100"/>
        <Label Content="R5 :" Width="30" Margin="20"/>
        <TextBox x:Name="R5" Height="25" Width="100"/>
        <Label Content="R6 :" Width="30" Margin="20"/>
        <TextBox x:Name="R6" Height="25" Width="100"/>
        <Button Content="Save" Width="80" Margin="20, 20" HorizontalAlignment="Right" Click="Button_Click"/>
    </StackPanel>
</Window>
